{
  // This file is not used in compilation. It is here just for a nice editor experience.
  "extends": "@docusaurus/tsconfig",
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-jsx",
    "types": [
      "react",
      "node"
    ],
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "exclude": [
    ".docusaurus",
    "build",
    "backend",
    "api",
    "pages/api",
    "pages/api/**",
    "pages/api/*",
    "pages/api/**/*"
  ]
}